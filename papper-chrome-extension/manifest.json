{
	"manifest_version": 2,
	"name": "Papper Chrome Extension",
	"description": "Работает со страницей в реальном времени",
	"version": "0.0.1",
	"icons": {"128": "logo_128.png"},
	"browser_action": {
		"default_icon": "logo_128.png",
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab", "tabs"],
	"content_scripts": [
		{
		  "matches": [
			"<all_urls>"
		  ],
		  "js": ["./scripts/popup.js"]
		}
	]
}